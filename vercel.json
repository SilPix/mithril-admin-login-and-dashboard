{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
        { "src": "/login", "dest": "/api/adminLogin.php" },
        { "src": "/dashboard", "dest": "/api/adminDashboard.php" },
        {"src": "/reg", "dest": "/api/registered.php" },
        {"src": "/test", "dest": "/api/emailTest.php" },
        { "src": "/(.*)",  "dest": "/api/index.php" }
    ]
}
